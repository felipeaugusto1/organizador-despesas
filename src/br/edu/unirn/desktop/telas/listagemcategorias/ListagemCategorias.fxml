<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="386.0" prefWidth="224.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.edu.unirn.desktop.telas.listagemcategorias.ListagemCategoriasController">
   <children>
      <TableView fx:id="tvCategorias" layoutX="12.0" layoutY="186.0" onMouseClicked="#selecionarCategoria" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="tcNome" prefWidth="75.0" text="Nome" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <TextField fx:id="txtNome" layoutX="14.0" layoutY="59.0" />
      <Button fx:id="btnSalvar" layoutX="17.0" layoutY="86.0" mnemonicParsing="false" onAction="#btnSalvarCategoria" text="Salvar" />
      <Button fx:id="btnDeletar" layoutX="14.0" layoutY="113.0" mnemonicParsing="false" onAction="#btnDeletarCategoria" text="Deletar" />
      <Button layoutX="16.0" layoutY="140.0" mnemonicParsing="false" onAction="#btnLimparCampos" text="Limpar" />
   </children>
</AnchorPane>
